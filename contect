import javax.swing.*;
import javax.swing.table.DefaultTableModel;
import java.awt.*;
import java.util.ArrayList;
import java.util.List;

// Step 1: Define the custom class
class Person {
    private int id;
    private String name;
    private int age;

    public Person(int id, String name, int age) {
        this.id = id;
        this.name = name;
        this.age = age;
    }

    public int getId() {
        return id;
    }

    public String getName() {
        return name;
    }

    public int getAge() {
        return age;
    }
}

public class CustomListTableExample {
    public static void main(String[] args) {
        // Step 2: Create a list of custom objects
        List<Person> personList = new ArrayList<>();
        personList.add(new Person(1, "John Doe", 25));
        personList.add(new Person(2, "Jane Smith", 30));
        personList.add(new Person(3, "Mike Johnson", 28));
        personList.add(new Person(4, "Emily Davis", 22));

        // Step 3: Create the table model and set column names
        String[] columnNames = { "ID", "Name", "Age" };
        DefaultTableModel tableModel = new DefaultTableModel(columnNames, 0);

        // Add data to the table model
        for (Person person : personList) {
            Object[] rowData = { person.getId(), person.getName(), person.getAge() };
            tableModel.addRow(rowData);
        }

        // Step 4: Create the table with the table model
        JTable table = new JTable(tableModel);

        // Step 5: Add the table to a scroll pane
        JScrollPane scrollPane = new JScrollPane(table);

        // Step 6: Create a new JFrame and add the scroll pane to it
        JFrame frame = new JFrame("Custom List Table Example");
        frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        frame.setSize(600, 400);
        frame.add(scrollPane, BorderLayout.CENTER);

        // Set the frame to be visible
        frame.setVisible(true);
    }
}

import javax.swing.*;
import java.awt.*;
import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

public class DropDownExample {
    public static void main(String[] args) {
        // Create a new JFrame
        JFrame frame = new JFrame("Drop Down Example");
        frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        frame.setSize(400, 200);

        // Create a panel to hold the combo box
        JPanel panel = new JPanel();
        frame.add(panel);

        // Create a label to display the selected item
        JLabel label = new JLabel("Selected: None");
        panel.add(label);

        // Create an array of strings to be used as items in the combo box
        String[] items = { "Item 1", "Item 2", "Item 3", "Item 4" };

        // Create the combo box with the items array
        JComboBox<String> comboBox = new JComboBox<>(items);

        // Add an ActionListener to handle item selection events
        comboBox.addActionListener(new ActionListener() {
            @Override
            public void actionPerformed(ActionEvent e) {
                // Get the selected item and update the label
                String selectedItem = (String) comboBox.getSelectedItem();
                label.setText("Selected: " + selectedItem);
            }
        });

        // Add the combo box to the panel
        panel.add(comboBox);

        // Set the frame to be visible
        frame.setVisible(true);
    }
}
