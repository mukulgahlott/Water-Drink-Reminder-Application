import javax.swing.*;
import javax.swing.table.DefaultTableModel;
import java.awt.*;
import java.util.ArrayList;
import java.util.List;

public class TableExample {
    public static void main(String[] args) {
        // Create a new JFrame
        JFrame frame = new JFrame("Table Example");
        frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        frame.setSize(600, 400);

        // Create a panel to hold the table
        JPanel panel = new JPanel(new BorderLayout());
        frame.add(panel);

        // Define column names
        String[] columnNames = { "ID", "Name", "Age" };

        // Create a list of data
        List<Object[]> dataList = new ArrayList<>();
        dataList.add(new Object[] { 1, "John Doe", 25 });
        dataList.add(new Object[] { 2, "Jane Smith", 30 });
        dataList.add(new Object[] { 3, "Mike Johnson", 28 });
        dataList.add(new Object[] { 4, "Emily Davis", 22 });

        // Create the table model and set column names
        DefaultTableModel tableModel = new DefaultTableModel(columnNames, 0);

        // Add data to the table model
        for (Object[] rowData : dataList) {
            tableModel.addRow(rowData);
        }

        // Create the table with the table model
        JTable table = new JTable(tableModel);

        // Add the table to a scroll pane
        JScrollPane scrollPane = new JScrollPane(table);
        panel.add(scrollPane, BorderLayout.CENTER);

        // Set the frame to be visible
        frame.setVisible(true);
    }
}

import javax.swing.*;
import java.awt.*;
import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

public class DropDownExample {
    public static void main(String[] args) {
        // Create a new JFrame
        JFrame frame = new JFrame("Drop Down Example");
        frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        frame.setSize(400, 200);

        // Create a panel to hold the combo box
        JPanel panel = new JPanel();
        frame.add(panel);

        // Create a label to display the selected item
        JLabel label = new JLabel("Selected: None");
        panel.add(label);

        // Create an array of strings to be used as items in the combo box
        String[] items = { "Item 1", "Item 2", "Item 3", "Item 4" };

        // Create the combo box with the items array
        JComboBox<String> comboBox = new JComboBox<>(items);

        // Add an ActionListener to handle item selection events
        comboBox.addActionListener(new ActionListener() {
            @Override
            public void actionPerformed(ActionEvent e) {
                // Get the selected item and update the label
                String selectedItem = (String) comboBox.getSelectedItem();
                label.setText("Selected: " + selectedItem);
            }
        });

        // Add the combo box to the panel
        panel.add(comboBox);

        // Set the frame to be visible
        frame.setVisible(true);
    }
}
